pipeline {

    agent any

    stages {
        
        stage("build") {

            steps {
                echo 'building my application...'
            }
        }

        stage("deploy") {

            steps {
              echo 'deploying my application...'
            }
        }

        stage("test") {

            steps {
                echo 'test my application...'
            }
        }
        stage("release") {

            steps {
                echo 'release my application...' 
            }
        }

    }
}